                    second: 'HH:mm:ss',
                    minute: 'HH:mm',
                    hour: 'HH:mm',
                    day: 'HH:mm',
                    week: 'HH:mm',
                    month: 'HH:mm',
                    quarter: 'HH:mm',
                    year: 'HH:mm'
                };
                config.options.scales.x.time.tooltipFormat = 'dd-MM-yyyy HH:mm:ss';
                
